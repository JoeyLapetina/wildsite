<h1>Listing submissions</h1>

<table>
  <tr>
    <th>Submission type</th>
    <th>Submission url</th>
    <th>Submission description</th>
    <th>Submitter name</th>
    <th>Submitter email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @submissions.each do |submission| %>
  <tr>
    <td><%= submission.submission_type %></td>
    <td><%= submission.submission_url %></td>
    <td><%= submission.submission_description %></td>
    <td><%= submission.submitter_name %></td>
    <td><%= submission.submitter_email %></td>
    <td><%= link_to 'Show', submission %></td>
    <td><%= link_to 'Edit', edit_submission_path(submission) %></td>
    <td><%= link_to 'Destroy', submission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Submission', new_submission_path %>
